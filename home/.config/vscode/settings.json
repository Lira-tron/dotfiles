  "vim.leader": "<space>",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#C34043",
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-c>"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "c"],
      "commands": ["editor.action.commentLine"],
      "silent": true
    }
  ],
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-o>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": ["C"],
      "after": ["c", "$"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<c-k>"],
      "commands": ["cursorPageUp"]
    },
    {
      "before": ["<c-j>"],
      "commands": ["cursorPageDown"]
    },
    {
      "before": ["<c-p>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<c-f>"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<leader>", "t", "e", "r", "m"],
      "commands": ["terminalHere.create"],
      "silent": true
    },
    {
      "before": ["<leader>", "o", "l", "d"],
      "commands": ["workbench.action.openRecent"],
      "silent": true
    },
    {
      "before": ["t", "l"],
      "commands": ["workbench.action.nextEditor"],
      "silent": true
    },
    {
      "before": ["t", "h"],
      "commands": ["workbench.action.previousEditor"],
      "silent": true
    },
    {
      "before": ["t", "d"],
      "commands": ["workbench.action.closeActiveEditor"],
      "silent": true
    },
    {
      "before": ["t", "n"],
      "commands": ["workbench.action.files.newUntitledFile"],
      "silent": true
    },
    {
      "before": ["f", "f"],
      "commands": ["fzf-quick-open.runFzfSearchPwd"],
      "silent": true
    },
    {
      "before": ["t", "t"],
      "commands": ["fzf-quick-open.runFzfFilePwd"],
      "silent": true
    },
    {
      "before": ["<leader>", "n"],
      "commands": [":noh"],
      "silent": false
    },
    {
      "before": ["<leader>", "l", "g"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": ["lazygit"]
        }
      ],
      "silent": false
    }
  ],
