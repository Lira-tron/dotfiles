{
  "bash": {
    "prefix": "bash",
    "body": ["```bash", "$1", "```"],
    "description": "Insert a bash code block"
  },

  "yaml": {
    "prefix": "yaml",
    "body": ["```yaml", "$1", "```"],
    "description": "Insert a yaml code block"
  },

  "go": {
    "prefix": "go",
    "body": ["```go", "$1", "```"],
    "description": "Insert a go code block"
  },

  "java": {
    "prefix": "java",
    "body": ["```java", "$1", "```"],
    "description": "Insert a kotlin code block"
  },

  "kotlin": {
    "prefix": "kotlin",
    "body": ["```kotlin", "$1", "```"],
    "description": "Insert a kotlin code block"
  },

  "sql": {
    "prefix": "sql",
    "body": ["```sql", "$1", "```"],
    "description": "Insert a sql code block"
  },

  "markdownlint": {
    "prefix": "markdownlint",
    "body": [
      " ",
      "<!-- markdownlint-disable -->",
      " ",
      "> $1",
      " ",
      "<!-- markdownlint-restore -->"
    ],
    "description": "Markdownlint disable range"
  },

  "prettierignore": {
    "prefix": "prettier-ignore",
    "body": [
      " ",
      "<!-- prettier-ignore-start -->",
      " ",
      "> $1",
      " ",
      "<!-- prettier-ignore-end -->"
    ],
    "description": "Prettier ignore range"
  },

  "linkt": {
    "prefix": "linkt",
    "body": ["[$1]($2){:target=\"_blank\"}"],
    "description": "Link with target"
  },

  "todo": {
    "prefix": "todo",
    "body": ["<!-- TODO: $1 -->"],
    "description": "Create a todo item"
  }

}
